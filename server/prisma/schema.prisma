// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model usertable{
  id Int @id @default(autoincrement())
  email String @unique
  name String
  password String
  phone String
  code String
  status String
}

model events{
  id Int @id @default(autoincrement())
  title String @unique
  start_event DateTime
  end_event DateTime
  calendar_n Int
}